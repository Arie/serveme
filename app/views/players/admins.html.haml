- content_for(:title) { "Current Players" }

= turbo_stream_from "admin-players"

#admin-players-content
  = render "admin_players_content", servers_with_players: @servers_with_players, distance_unit: @distance_unit

:javascript
  function initializeTooltips() {
    $('.asn-tooltip').tooltip({html: true});
  }
  
  document.addEventListener('turbo:load', initializeTooltips);
  document.addEventListener('turbo:render', initializeTooltips);