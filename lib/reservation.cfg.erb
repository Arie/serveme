hostname "<%= server_name %>"
rcon_password "<%= rcon %>"
sv_password "<%= password %>"
tv_password "<%= tv_password %>"
tv_relaypassword "<%= tv_relaypassword %>"
sv_logflush 0
sv_logbans 1
logaddress_add <%= "direct.#{SITE_HOST}:40001" %>
sv_rcon_whitelist_address "<%= whitelist_ip %>"

<% if server_config %>
<%= "exec #{server_config}" %><% end %>
<% if custom_whitelist_id %>
<%= %{mp_tournament_whitelist "cfg/custom_whitelist_#{custom_whitelist_id}.txt"} %>
<%= %{say "Whitelist: http://whitelist.tf/#{custom_whitelist_id}"} %>
<% elsif whitelist %>
<%= %{mp_tournament_whitelist "cfg/#{whitelist}"} %><% end %>

<% if user.logs_tf_api_key.present? %>
  <%= %{logstf_apikey "#{user.logs_tf_api_key}"} %>
<% else %>
  <%= %{logstf_apikey "#{LOGS_TF_API_KEY}"} %>
<% end %>
logstf_title "serveme.tf - {red} vs {blu}"

tv_delaymapchange 1
tv_enable 1
<% if enable_demos_tf? %>
<%= "tv_autorecord 0" %>
<%= %{sm_demostf_apikey "#{DEMOS_TF_API_KEY}"} %>
<% else %>
<%= "tv_autorecord 1" %>
<% end %>

tf_preround_push_from_damage_enable 1

